{{#if record.specialRequirements}}
  <p><strong>{{t 'form-cfregistration-specialRequirements'}}</strong></p>
  <p>{{record.specialRequirements}}</p>
{{/if}}

<p>
  <strong>ID de inscrição:</strong>{{id}}<br>
  <strong>Inscrito em:</strong>{{we-date record.createdAt}}<br>
</p>

{{#with record.user}}
<div>
  <table class="table table-striped table-hover">
    <tbody>
      <tr><th>ID:</th><td>{{id}}</td></tr>
      <tr><th>Nome de exibição:</th><td>{{displayName}}</td></tr>
      <tr><th>Nome completo:</th><td>{{fullName}}</td></tr>
      <tr><th>email:</th><td>{{email}}</td></tr>
      <tr><th>CPF:</th><td>{{cfp}}</td></tr>
      <tr><th>Passaporte:</th><td>{{passport}}</td></tr>
      <tr><th>Registrado em:</th><td>{{we-date createdAt}}</td></tr>
    </tbody>
  </table>
</div>
{{/with}}