{{#widget-wrapper widget}}
<div class="theses">
  <div class="container">
    <div class="row text-center">
    {{#each conference.topics}}
      <div class="col-md-4 wow bounceIn" data-wow-delay=".4s">
        <div class="icon-wrap">
          {{#each image}}
            <img src="{{urls.original}}">
          {{/each}}
        </div>
        <h3>{{title}}</h3>
        <hr>
        <p>{{{about}}}</p>
      </div>
    {{else}}
      <div class="col-md-4 wow bounceIn" data-wow-delay=".4s">
        <div class="icon-wrap">
          <img src="/public/theme/we-theme-conference-seminarioeducacao.fiocruz.br/img/icone_educacao.png">
        </div>
        {{!-- <h3>Eixo1</h3> --}}
        <hr>
        <p>Educação, Tecnologia e Sociedade:<br> qual futuro queremos? </p>
      </div>
      <div class="col-md-4 wow bounceIn" data-wow-delay=".6s">
        <div class="icon-wrap">
          <img src="/public/theme/we-theme-conference-seminarioeducacao.fiocruz.br/img/icone_saude.png">
        </div>
        {{!-- <h3>Eixo2</h3> --}}
        <hr>
        <p>Educação e Trabalho para o SUS:<br> desafios de um projeto de país</p>
      </div>
      <div class="col-md-4 wow bounceIn" data-wow-delay=".8s">
        <div class="icon-wrap">
          <img src="/public/theme/we-theme-conference-seminarioeducacao.fiocruz.br/img/icone_sociedade.png">
        </div>
        {{!-- <h3>Eixo3</h3> --}}
        <hr>
        <p>Formação para a Pesquisa e Inovação</p>
      </div>
    {{/each}}


    </div>
  </div>
</div>
{{/widget-wrapper}}