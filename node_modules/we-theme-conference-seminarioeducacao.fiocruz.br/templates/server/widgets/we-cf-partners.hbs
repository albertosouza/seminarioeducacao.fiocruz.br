<div class="partners">
  <div class="container">
    <div class="section-heading">
      <h1>{{widget.title}}</h1>
    </div>
    <div class="row text-center">
      <div class="col-lg-12">
        <ul class="list-inline">
          {{#each widget.partners}}
            <li>
            {{#if link}}
              <a target="_blank" href="{{link}}">{{#each logo}}{{we-image this 'original'}}{{/each}}</a>
            {{else}}
              {{#each logo}}{{we-image this 'original'}}{{/each}}
            {{/if}}
            </li>
          {{/each}}
        </ul>
      </div>
    </div>
  </div>
</div>