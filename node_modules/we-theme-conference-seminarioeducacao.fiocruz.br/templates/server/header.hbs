<!-- ///  begin header  /// -->
<header class="main-header">
  <nav class="navbar navbar-custom navbar-fixed-top" role="navigation">
    {{!-- barra brasil --}}
    <div id="barra-brasil" style="background:#7F7F7F; height: 20px; padding:0 0 0 10px;display:block;">
    <ul id="menu-barra-temp" style="list-style:none;">
      <li style="display:inline; float:left;padding-right:10px; margin-right:10px; border-right:1px solid #EDEDED"><a href="http://brasil.gov.br" style="font-family:sans,sans-serif; text-decoration:none; color:white;">Portal do Governo Brasileiro</a></li>
      <li><a style="font-family:sans,sans-serif; text-decoration:none; color:white;" href="http://epwg.governoeletronico.gov.br/barra/atualize.html">Atualize sua Barra de Governo</a></li>
    </ul>
    </div>
    <script defer="defer" src="//barra.brasil.gov.br/barra.js" type="text/javascript"></script>
    {{!-- /barra brasil --}}

    <div class="container">
      <!-- Brand and toggle get grouped for better mobile display -->
      <div class="navbar-header">
        <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#navbar-collapse">
          <span class="sr-only">Toggle navigation</span>
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
        </button>
        <a class="navbar-brand" href="/">
          {{#each conference.logo}}
            <img class="conference-logo" src="{{urls.original}}">
          {{else}}
            <img class="conference-logo" src="/public/theme/we-theme-conference-seminarioeducacao.fiocruz.br/img/logo-seminario-edicacao.png">
          {{/each}}
        </a>
      </div>
      <!-- Collect the nav links, forms, and other content for toggling -->
      <div class="collapse navbar-collapse" id="navbar-collapse">
        <ul class="nav navbar-nav navbar-right">{{we-user-menu}}</ul>
        {{{we-cf-menu conference.mainMenu}}}
      </div><!-- /.navbar-collapse -->
    </div><!-- /.container-fluid -->
  </nav>
</header>
<!-- ///  end header  /// -->